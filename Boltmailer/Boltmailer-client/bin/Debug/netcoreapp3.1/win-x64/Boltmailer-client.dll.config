<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- The directory in which the program attempts to read the files from. Server value should match this. -->
    <!-- This should be the location of the SMB server plus a prefix. Example: "\\Desktop-m9c97vk\f\Boltmailer" -->
    <add key="employeesRootPath" value="C:\Boltmailer" />
    
    <!-- Only change values below, if you know what you're doing.. -->
    <add key="employeeSearchTerm" value="" />
    <add key="showCompletedProjects" value="false" />
    <add key="sendNotifications" value="true" />
    <add key="alwaysOnTop" value="false" />
    <add key="adminMode" value="false" />
    <add key="setupCompleted" value="false" />
  </appSettings>
</configuration>